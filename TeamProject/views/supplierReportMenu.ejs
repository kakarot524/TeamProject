            <script>
               var app = angular.module("supplierReportMenu", ["ngRoute"]);
                app.config(function($routeProvider) {
                $routeProvider
                    .when("/totalSupplier", {
                        templateUrl : "totalSupplier"
                    })
                    .when("/supplierOptions",{
                        templateUrl: "supplierOptions"
                    })
                     .when("/supplierCost",{
                        templateUrl: "supplierCost"
                    })
                     .otherwise({
                        templateUrl:"totalSupplier"
                    })
                     
                  
                });
            </script>

